public class MinimumCoins {
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
	int t = sc.nextInt();
	while(t-->0){
	  int X = sc.nextInt();
	  if(X%10==0){
	    System.out.println(X/10);  
	  } else if(X%10==5) {
	    System.out.println((X/10)+1);   
	  } else {
	    System.out.println(-1);   
	  }
	}
  }
}