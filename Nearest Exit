import java.util.*;
public class NearestExit {
   public static void main(String args[]){ 
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int X = sc.nextInt();
      if(X<=50){
        System.out.println("LEFT");  
      } else {
        System.out.println("RIGHT");  
      }
    }
   }
}