import java.util.*;
public class Playlist{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int N = sc.nextInt(); 
      int X = sc.nextInt();
      int a = N/X;
      int b = a/3;
      System.out.println(b);
    }
  }
}