import java.util.*;
public class ProfitIncrement {
   public static void main(String args[]){   
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
     while(t-->0){
      int X = sc.nextInt();
      int Y = sc.nextInt();
      int A = X/10;
      int B = A+X;
      int C = X-Y;
      System.out.println(B-C);
    }
  }
}