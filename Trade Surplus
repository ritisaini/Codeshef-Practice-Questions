public class TradeSurplus{
  public static void main(String args[]){
      Scanner sc = new Scanner(System.in);
      int t = sc.nextInt();
      while(t-->0){
      int A1 = sc.nextInt();
      int A2 = sc.nextInt();
      int B1 = sc.nextInt();
      int B2 = sc.nextInt();
      int C = (A1-A2);
      int D = B1-B2;
      int E = C+D;
      if(E>=0){
        System.out.println("NO");  
      } else {
        System.out.println("YES");  
      }
    }
    }
}