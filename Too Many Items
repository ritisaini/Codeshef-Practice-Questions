public class TooItems {
   public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int N = sc.nextInt();
      if(N%10==0){
        System.out.println(N/10);  
      } else {
        System.out.println((N/10)+1);  
      }
    }
   }   
}
