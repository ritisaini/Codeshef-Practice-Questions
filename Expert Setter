import java.util.*;
public class ExpertSetter {
  public static void main(String args[]){   
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      float X = sc.nextInt();
      float Y = sc.nextInt();
      float Z = X/2;
      if(Y >= Z){
        System.out.println("YES");  
      } else {
        System.out.println("NO");  
      }
    }
  }
}