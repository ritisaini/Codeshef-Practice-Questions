import java.util.*;
public class GreaterAverage {
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      float A = sc.nextInt();
      float B = sc.nextInt();
      float C = sc.nextInt();
      float X = (A+B)/2;
      if(X>C){
        System.out.println("YES");  
      } else {
        System.out.println("NO");  
      }
    }
  }
}
