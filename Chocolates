import java.util.*;
public class Chocolates {
   public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int C = sc.nextInt();
      int X = sc.nextInt();
      int Y = sc.nextInt();
      int A = (C-X)*Y;
        System.out.println(A);  
    }
  }
}