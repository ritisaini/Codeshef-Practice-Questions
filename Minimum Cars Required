public class MinimumCars {
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int N = sc.nextInt();
      if(N%4==0){
        System.out.println(N/4);  
      } else {
        System.out.println((N/4)+1);
      }
    }
    }
}
