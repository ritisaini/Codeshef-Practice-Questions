public class PendingAssignment {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0){
        int X = sc.nextInt();
        int Y = sc.nextInt();
        int Z = sc.nextInt();
        int a = X*Y;
        int b = Z*24*60;
         if(a<=b){
           System.out.println("YES");      
         } else {
           System.out.println("NO");  
         }
        }
    }
}
