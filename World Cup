public class WorldCup {
  public static void main(String args[]){ 
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int X = sc.nextInt();
      int Y = sc.nextInt();
      int Z = sc.nextInt();
      int A = Math.abs(X-Y);
      if(A<=Z){
        System.out.println("YES");	        
      } else {
         System.out.println("NO");	  
      }
    }
  }
}
