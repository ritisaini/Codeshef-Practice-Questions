import java.util.*;
public class Subscription {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
	    int t = sc.nextInt();
	    while(t-->0){
	     int N = sc.nextInt();
	     int X = sc.nextInt();
	     int S = (N+5)/6;
	      System.out.println(S*X);
	    }
    }
}