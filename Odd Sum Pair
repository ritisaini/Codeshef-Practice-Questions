import java.util.*;
public class ODDSumPair {
  public static void main(Stringa args[]){
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int A = sc.nextInt();
      int B = sc.nextInt();
      int C = sc.nextInt();
      int a = A+B;
      int b = B+C;
      int c = A+C;
     if((a%2==1) || (b%2==1) || (c%2==1)){
       System.out.println("YES");     
     } else {
       System.out.println("NO");     
     }
	}
    }
}
