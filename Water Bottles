public class WaterBottles {
  public static void main(String args[]){    
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
    int N = sc.nextInt();
    int X = sc.nextInt();
    int K = sc.nextInt();
      if(X>K){
        System.out.println(0);  
      } else if(N*X<=K){
        System.out.println(N);   
      } else {
        System.out.println(K/X);    
      }    
    }
  }
}