import java.util.*;
public class PizzaCutting {
  public static void main(String args[]){   
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int N = sc.nextInt();
      if(N % 2 == 0 || N == 1){
        System.out.println("YES");  
      } else {
        System.out.println("NO");  
      }
    }
  }
}