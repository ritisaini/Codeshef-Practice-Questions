import java.util.*;
public class TestScore {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0){
          int N = sc.nextInt();
          int X = sc.nextInt();
          int Y = sc.nextInt();
          if(Y%X==0){
            System.out.println("YES");  
          } else {
            System.out.println("NO");  
          }
        }
    }
}