import java.util.*;
public class MonopolySorting {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0)
		{
		    int A[] = new int[4];
		    for(int i=0; i<4; i++)
		    A[i]=sc.nextInt();
		    Arrays.sort(A);
		    if(A[0]+A[1]+A[2]<A[3]){
		    System.out.println("YES");
		    } else {
		    System.out.println("NO");
		    }
		}
    }
}