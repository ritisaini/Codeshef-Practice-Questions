public class ChessRating {
  public static void main(String args[]){   
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int X = sc.nextInt();
      int Y = sc.nextInt();
      int C = Y-X;
      if(X==Y){
        System.out.println("0");  
      } else {
          if(C%8==0){
            System.out.println(C/8);  
          } else {
            System.out.println((C/8)+1);  
          }
      }
    }
  }
}