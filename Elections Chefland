import java.util.*;
public class ElectionsChefland {
  public static void main(String args[]){   
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int N = sc.nextInt();
      int X = sc.nextInt();
      int count = 0;
      int A[] = new int[N];
      for(int i=0; i<N; i++){
        A[i] = sc.nextInt();
         if(A[i]>=X){
           count++;     
         }
      }
      System.out.println(count);
    }
  }
}