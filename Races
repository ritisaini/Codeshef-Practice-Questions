public class Races {
  public static void main(String args[]){ 
    Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		for(int i=0;i<t;i++){
		    int a=sc.nextInt();
		    int b=sc.nextInt();
		    int c=sc.nextInt();
		    int d=sc.nextInt();
		    int count=2;
		    if((c==a)||(c==b)){
		        count=count-1;
		    }
		    if((d==a)||(d==b)){
		        count=count-1;
		    }
		    System.out.println(count);
        }
    }
}

