import java.util.*;
public class ChangeRowColumn{
  public static void main(String args[]){   
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int Sx = sc.nextInt();
      int Sy = sc.nextInt();
      int Ex = sc.nextInt();
      int Ey = sc.nextInt();
      if(Sx!=Ex && Sy!=Ey){
        System.out.println("1");      
      } else {
        System.out.println("2");    
    }
	}
  }
}