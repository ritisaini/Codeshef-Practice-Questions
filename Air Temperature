public class AirTemp {
  public static void main(String args[]){ 
    Scanner sc = new Scanner(System.in);
	 int t = sc.nextInt();
	 for(int i=1; i<=t; i++){
	 int A = sc.nextInt();
	 int B = sc.nextInt();
	 int C = sc.nextInt();
	 if(B>=A && B>=C){
	   System.out.println("YES");  
	 } else {
	   System.out.println("NO");  
	 }
	}
  }
}