public class Valentine {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0){
          int X = sc.nextInt();
          int Y = sc.nextInt();
          if(X<Y){
            System.out.println("0");  
          } else {
            System.out.println(X/Y);    
          }
        }
    }
}
