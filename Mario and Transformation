public class MarioTransformation {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		while(T-->0){
		    int X = sc.nextInt();
		    if(X%3==0){
		        System.out.println("NORMAL");
		    }else if(X%3==1){
		        System.out.println("Huge");
		    }else{
		        System.out.println("Small");
		    }
		}
    }
}
