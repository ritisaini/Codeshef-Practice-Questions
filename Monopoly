import java.util.*;
public class Monopoly {
   public static void main(String args[]){  
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int R1 = sc.nextInt();
      int R2 = sc.nextInt();
      int R3 = sc.nextInt();
      int A = R1+R2;
      int B = R2+R3;
      int C = R1+R3;
      if(A < R3 || B < R1 || C < R2){
        System.out.println("YES");  
      } else {
        System.out.println("NO");  
      }
    }
  }
}