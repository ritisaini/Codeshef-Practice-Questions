import java.util.*;
public class ChefandGYM {
   public static void main(String args[]){  
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int X = sc.nextInt();
      int Y = sc.nextInt();
      int Z = sc.nextInt();
      if(X+Y <= Z){
        System.out.println("2");      
      } else if((X <= Z) && (X+Y > Z)){
        System.out.println("1");  
      } else {
        System.out.println("0");  
      }
    }
   }
}